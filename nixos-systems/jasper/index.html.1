		function initialStatus()
		{
			wanConnUpIdx=0;

			if (wan_active_idx.length !=0)
				wanConnUpIdx = parseInt(wan_active_idx,10)+1;

			for(var i=0;i<wan_conn_status_list.length-1;i++) //length-1 is because null item
			{
				var wan_status = HTML2str(wan_conn_status_list[i]);
				if (wan_status.indexOf("connected")==0)
				{
					wanConnStatus[i]=1;
				}
				else
				{
					wanConnStatus[i]=0;
				}
			}

			if(wanConnUpIdx>0 && wanConnUpIdx<=3)
			{
				setIdVal("status_connectionStatus","Connected");
				setIdVal("m_status_connectionStatus","Connected");

				//change background color
				$('#status_box').removeClass('backgroundYellow').addClass('backgroundClass');
				document.getElementById("basicStatus").style.color = "#ffffff";
				document.getElementById("status_connectionStatus").style.color = "#ffffff";
				document.getElementById("basicStatusTi").style.color = "#ffffff";
				DisplayObject("status_icon_img");
				HiddenObject("status_icon_img2");
