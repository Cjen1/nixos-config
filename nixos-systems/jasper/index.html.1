				setIdVal("m_pause", "Off");
			}

			var default_group_block = false;
			var group_enable = 0;
			var countedGroup = [];
			for(var i = 0; acl_list[i] != null; i++) {
				// if Default group has "active" schedule rule or pause rule
				if(unescape(acl_list[i].pid) == "default_profile" && acl_list[i].enable == '1') {
					default_group_block = true;
				}

				//schedule type=2, pause type=1
				//schedule and pause should be counted if its enable is 1
				if(acl_list[i].pid[0] == "g" && /*acl_list[i].type == '2' &&*/ acl_list[i].enable == '1') {
					//if group has schedule and pause rule at same time, the acl_list will include 2 records.
					if(countedGroup.indexOf(acl_list[i].pid) != -1){
						continue;
					}
					else{
						group_enable++;
						countedGroup.push(acl_list[i].pid);
					}
				}
			}

			if(default_group_block == true){
				setIdVal("ACL_en", "On");
				setIdVal("m_ACL_en", "On");
			}
			else{
				setIdVal("ACL_en", "Off");
				setIdVal("m_ACL_en", "Off");
