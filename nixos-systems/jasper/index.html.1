				document.getElementById("moblie_status").style.color = "#ffffff";
				document.getElementById("moblie_statusTi").style.color = "#ffffff";
				DisplayObject("moblie_statusImg");
				HiddenObject("moblie_statusImg2");
			}
			else
			{
				setIdVal("status_connectionStatus","Disconnected");
				setIdVal("m_status_connectionStatus","Disconnected");

				//change background color
				$('#status_box').removeClass('backgroundClass').addClass('backgroundYellow');
				document.getElementById("basicStatus").style.color = "#333333";
