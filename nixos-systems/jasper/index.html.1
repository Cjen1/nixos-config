		function UsbDevice(usbListItem)
		{
			this.listItem = usbListItem;
			this.getStatus = function()
			{
				if (this.listItem.status == "connected")
					return "Connected";
				else
					return "Not connected";
			};

			this.isConnected = function()
			{
				return (this.listItem.status == "connected") ? true : false;
			};

			this.isDisconnected = function()
			{
				return (this.listItem.status == "disconnected") ? true : false;
			};

			this.isRemoved = function()
			{
				return (this.listItem.status == "removed") ? true : false;
			};
		}

